valor=int(input('Quanto vocÃª quer sacar? R$'))
c50=valor//50
c20=(valor%50)//20
c10=((valor%50)%20)//10
c1=((valor%50)%20)%10
if c50>0:
    print(f'Total de {c50} notas de 50$')
if c20>0:
    print(f'Total de {c20} notas de 20$')
if c10>0:
    print(f'Total de {c10} notas de 10$')
if c1>1:
    print(f'Total de {c1} moedas de 1$')
